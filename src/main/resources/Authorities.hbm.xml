<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="seopays.domain">
	<class name="Authorities" table="authorities">
        <id name="id">
            <generator class="native" />
        </id>

        <property name="username" column="username" type="string" length="50"></property>
		<property name="authority" column="authority" type="string" length="50"></property>
	</class>
</hibernate-mapping>